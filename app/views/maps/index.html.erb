<p id="notice"><%= notice %></p>

<h1>Maps</h1>

<table>
  <thead>
    <tr>
      <th>Nome trecho</th>
      <th>Numero lei</th>
      <th>Latitude inicial</th>
      <th>Longitude inicial</th>
      <th>Latitude final</th>
      <th>Longitude final</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maps.each do |map| %>
      <tr>
        <td><%= map.nome_trecho %></td>
        <td><%= map.numero_lei %></td>
        <td><%= map.latitude_inicial %></td>
        <td><%= map.longitude_inicial %></td>
        <td><%= map.latitude_final %></td>
        <td><%= map.longitude_final %></td>
        <td><%= link_to 'Show', map %></td>
        <td><%= link_to 'Edit', edit_map_path(map) %></td>
        <td><%= link_to 'Destroy', map, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>


  </tbody>

</table>

<br>

<%= link_to 'New Map', new_map_path %>
